

    <div id="content">

		<%= form_for [:admin, @subject_info] , :html => { :multipart => true } do |f| %>
		  
		  <div id="breadcrumb">
		  	   <%= link_to "Subject Information", admin_subject_info_path %> &raquo; New
		  </div>
		 
		  <p>
		    <%= f.label :title %>
		    <%= f.text_field :title %>
		  </p>
		  
		
		    <%= f.label :body %>
		    <%= f.cktext_area :body,:height=>400, :width=>900 %>
		  </p>
		
		  
		  <%= render :partial =>"button" %>
		
		<% end %>


